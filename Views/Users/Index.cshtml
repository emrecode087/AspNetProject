@model IEnumerable<AppUser>

<a class="btn btn-primary" asp-action="Create">Create User</a>
<hr />

<table class="table table-sm table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>FullName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FullName</td>
                <td>@user.Email</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@user.Id" class="">Edit</a>
                    <a href="" class="">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
